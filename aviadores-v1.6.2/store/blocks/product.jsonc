  //FIN PRODUCTOS RELACIONADOS
  

  "shelf.relatedProducts#sugeridos":{
    "blocks": ["product-summary.shelf#vitrina-product"],
    "props":{
      "recommendation": "buy",
       "productList": {
          
            "titleText": "Productos similares",
            "maxItems": 10,
            "itemsPerPage": 4.2,
            "infinite": "true",
            "scroll": "BY_PAGE",
            "arrows": "true",
           "minItemsPerPage": 1.5,
           "gap":"ph2"

        },
  
        "blockClass": "shelf-vitrina"
    }
  },
  
  "shelf.relatedProducts#similares":{
    "blocks": ["product-summary.shelf#vitrina-product"],
    "props":{
      "recommendation": "view",
       "productList": {
        "maxItems": 10,

            "titleText": "Podría ser de su interés",
            "itemsPerPage": 4.2,
            "scroll": "BY_PAGE",
            "arrows": "true",
            "minItemsPerPage": 1.5,
            "gap":"ph2"
        },
  
        "blockClass": "shelf-vitrina"
    }
  }
              
