            "textPosition":"CENTER",
            "textAlignment":"LEFT",
            "blockClass":"paso-2"
        }
    },
    
    //FIN Paso 5

    // Paso 3
    "flex-layout.col#paso-6":{
        "props":{
            "marginLeft":4,
            "marginRight":4,
            "blockClass":"paso-3",
            "preventVerticalStretch":true
        },
        "children":[
            "responsive-layout.desktop#paso-6-desktop",
            "responsive-layout.mobile#paso-6-mobile",
            "flex-layout.row#paso-6-title",
            "rich-text#paso-6-paragraph"
            /* "rich-text#paso-6-links" */
