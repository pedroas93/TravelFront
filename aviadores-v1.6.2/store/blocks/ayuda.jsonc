    // Pasos para la compra 2do Row
    "flex-layout.row#pasos-compra2":{
        "props":{
            "preventVerticalStretch":true
        },
        "children":[
            "flex-layout.col#paso-4",
            "flex-layout.col#paso-5",
            "flex-layout.col#paso-6"
