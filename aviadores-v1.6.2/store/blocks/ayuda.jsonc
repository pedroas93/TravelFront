    "responsive-layout.desktop#paso-2-desktop":{
        "children":["image#paso-2-desktop"]
    },
    "responsive-layout.mobile#paso-2-mobile":{
        "children":["image#paso-2-mobile"]
    },

    "image#paso-2-desktop":{
        "props": {
            "src": "/arquivos/step--2.png",
            "blockClass":"paso-2-desktop"
          }
    },
    "image#paso-2-mobile":{
        "props": {
            "src": "/arquivos/step--2.png"
          }
    },

    "flex-layout.row#paso-2-title":{
        "props":{
            "blockClass":"paso-2-title"
        },
        "children":["rich-text#paso-2-title"]
    },

    "rich-text#paso-2-title":{
        "props":{
            "text":"![alt img](assets/ayuda/paso2.svg)&ensp;Agrégalo<br>&ensp;al carrito",
            "textPosition":"CENTER",
            "textAlignment":"CENTER",
            "font":"t-heading-5",
            "blockClass":"paso-2-title"
        }
    },
    /* "rich-text#paso-2-links":{
        "props":{
            "blockClass":"paso-2-links",
            "text":"[*1. ¿Puedo agregar y quitar libremente los productos del carrito?*](/ayuda/faqs)<br><br>[*2. ¿Puedo agregar un mismo producto en distintos talles o colores?*](/ayuda/faqs)<br><br>[*3. ¿Cómo puedo agregar un cupón de descuento?*](/ayuda/faqs)"
        }
    }, */
    //FIN Paso 2

    "rich-text#paso-2-paragraph":{
        "props":{
            "text":"Revisa los detalles de tu producto y \n agrégalo al carrito",
            "textPosition":"CENTER",
            "textAlignment":"LEFT",
            "blockClass":"paso-2"
        }
    },

    // Paso 3
    "flex-layout.col#paso-3":{
        "props":{
            "marginLeft":4,
            "marginRight":4,
            "blockClass":"paso-3",
            "preventVerticalStretch":true
        },
        "children":[
            "responsive-layout.desktop#paso-3-desktop",
            "responsive-layout.mobile#paso-3-mobile",
            "flex-layout.row#paso-3-title",
            "rich-text#paso-3-paragraph"
            /* "rich-text#paso-3-links" */
        ]
    },
    "responsive-layout.desktop#paso-3-desktop":{
        "children":["image#paso-3-desktop"]
    },
    "responsive-layout.mobile#paso-3-mobile":{
        "children":["image#paso-3-mobile"]
    },

    "image#paso-3-desktop":{
        "props": {
            "src": "/arquivos/step--3.png",
            "blockClass":"paso-3-desktop"
          }
