    "image#paso-3-mobile":{
        "props": {
            "src": "/arquivos/step--3.png"
          }
    },

    "flex-layout.row#paso-3-title":{
        "props":{
            "blockClass":"paso-3-title"
        },
        "children":["rich-text#paso-3-title"]
    },

    "rich-text#paso-3-title":{
        "props":{
            "text":"![alt img](assets/ayuda/paso2.svg)&ensp;Verifica tus <br> productos",
            "textPosition":"CENTER",
            "textAlignment":"CENTER",
            "font":"t-heading-5",
            "blockClass":"paso-3-title"
        }
    },

    "rich-text#paso-3-paragraph":{
        "props":{
            "text":"Verfica la cantidad en el carrito de compras \n y luego dirígite a “finalizar compra”.",
            "textPosition":"CENTER",
            "textAlignment":"LEFT",
            "blockClass":"paso-3"
        }
    },
