    },


    "responsive-layout.desktop#paso-4-desktop":{
        "children":["image#paso-4-desktop"]
    },
    "responsive-layout.mobile#paso-4-mobile":{
        "children":["image#paso-4-mobile"]
    },

    "image#paso-4-desktop":{
        "props": {
            "src": "/arquivos/step--4.png",
            "blockClass":"paso-1-desktop"
          }
    },
    "image#paso-4-mobile":{
        "props": {
            "src": "/arquivos/step--4.png"
          }
    },

    "flex-layout.row#paso-4-title":{
        "props":{
            "blockClass":"paso-1-title"
        },
