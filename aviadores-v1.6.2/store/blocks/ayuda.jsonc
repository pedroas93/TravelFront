            "rich-text#paso-5-paragraph"
            /* "rich-text#paso-5-links" */
        ]
    },
    "responsive-layout.desktop#paso-5-desktop":{
        "children":["image#paso-5-desktop"]
    },
    "responsive-layout.mobile#paso-5-mobile":{
        "children":["image#paso-5-mobile"]
    },

    "image#paso-5-desktop":{
        "props": {
            "src": "/arquivos/step--5.png",
            "blockClass":"paso-5-desktop"
          }
    },
    "image#paso-5-mobile":{
